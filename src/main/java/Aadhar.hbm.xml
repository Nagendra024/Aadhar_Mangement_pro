<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.Digit.Aahar_pro.bean.AadharDetails"
		table="aadhar">
		<id name="a_id" column="a_id"></id>
		<property name="u_name">
			<column name="u_name" />
		</property>
		<property name="address">
			<column name="address" />
		</property>
		<property name="pin">
			<column name="pin" />
		</property>
		<property name="phone_no">
			<column name="phone_no" />
		</property>
		<property name="dob">
			<column name="dob" />
		</property>
	</class>
</hibernate-mapping>